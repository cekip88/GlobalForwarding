                        .tab-block-item
                            .input
                                .input-row.input-row-img
                                    include ../img/weight.svg
                                    input(type='text' placeholder='Parcel weight in kg').input-text
                                span.input-error-text This field is required
                        button(type='button').btn-big.orange Get quote
